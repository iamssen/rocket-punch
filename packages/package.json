{
  "name": "packages",
  "version": "1.0.0",
  "scripts": {
    "build": "node -r ts-node/register -r tsconfig-paths/register scripts/build.ts",
    "publish": "node -r ts-node/register -r tsconfig-paths/register scripts/publish.ts",
    "release": "yarn run test && yarn run build && yarn run publish",
    "view": "node -r ts-node/register -r tsconfig-paths/register scripts/view.ts",
    "doctor": "node -r ts-node/register -r tsconfig-paths/register scripts/doctor.ts",
    "test": "jest --colors",
    "coverage": "jest --coverage",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/{**/*,*}.{ts,tsx}": [
      "jest --colors --findRelatedTests"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@types/babel__core": "^7.1.16",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.4",
    "@types/node": "^16.11.6",
    "@types/node-fetch": "^3.0.3",
    "@types/prettier": "^2.4.1",
    "@types/prompts": "^2.0.14",
    "@types/react": "^17.0.33",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.9",
    "@types/tmp": "^0.2.2",
    "@types/toposort": "^2.0.3",
    "@types/yargs": "^17.0.5",
    "chalk": "^4.1.2",
    "depcheck": "^1.4.2",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "jest": "^26.6.3",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.2.6",
    "mini-svg-data-uri": "^1.4.3",
    "node-fetch": "^3.0.0",
    "package-json": "^7.0.0",
    "prettier": "^2.4.1",
    "prompts": "^2.4.2",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "tmp": "^0.2.1",
    "toposort": "^2.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "type-fest": "^2.5.2",
    "typescript": "^4.4.4",
    "yargs": "^17.2.1"
  }
}
