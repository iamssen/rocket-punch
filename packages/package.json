{
  "name": "packages",
  "version": "1.0.0",
  "scripts": {
    "build": "node -r ts-node/register -r tsconfig-paths/register scripts/build.ts",
    "publish": "node -r ts-node/register -r tsconfig-paths/register scripts/publish.ts",
    "view": "node -r ts-node/register -r tsconfig-paths/register scripts/view.ts",
    "doctor": "node -r ts-node/register -r tsconfig-paths/register scripts/doctor.ts",
    "e2e": "npm run build && bash test/e2e.sh && rimraf out",
    "test": "jest --colors",
    "coverage": "jest --coverage",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/{**/*,*}.{ts,tsx}": [
      "jest --colors --findRelatedTests"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@svgr/plugin-jsx": "^5.4.0",
    "@types/babel__core": "^7.1.12",
    "@types/fs-extra": "^9.0.3",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.15",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.14.7",
    "@types/node-fetch": "^2.5.7",
    "@types/prettier": "^2.1.5",
    "@types/prompts": "^2.4.0",
    "@types/react": "^16.9.56",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.3.4",
    "@types/tmp": "^0.2.0",
    "@types/yargs": "^15.0.9",
    "chalk": "^4.1.0",
    "depcheck": "^1.3.1",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.5.1",
    "mini-svg-data-uri": "^1.2.3",
    "node-fetch": "^2.6.1",
    "package-json": "^6.5.0",
    "prettier": "^2.1.2",
    "prompts": "^2.4.0",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2",
    "tmp": "^0.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^0.18.0",
    "typescript": "^4.0.5",
    "yargs": "^16.1.0"
  }
}
