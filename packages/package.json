{
  "name": "packages",
  "version": "1.0.0",
  "scripts": {
    "build": "node -r ts-node/register -r tsconfig-paths/register scripts/build.ts",
    "publish": "node -r ts-node/register -r tsconfig-paths/register scripts/publish.ts",
    "release": "cross-env CI=true yarn run test && yarn run build && yarn run publish",
    "view": "node -r ts-node/register -r tsconfig-paths/register scripts/view.ts",
    "doctor": "node -r ts-node/register -r tsconfig-paths/register scripts/doctor.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "cross-env CI=true vitest related"
    ]
  },
  "devDependencies": {
    "@svgr/plugin-jsx": "^6.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.35",
    "@types/node-fetch": "^2.6.1",
    "@types/prettier": "^2.6.1",
    "@types/prompts": "^2.4.0",
    "@types/react": "^18.0.9",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.9",
    "@types/tmp": "^0.2.3",
    "@types/toposort": "^2.0.3",
    "@types/yargs": "^17.0.10",
    "c8": "^7.11.3",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.4.2",
    "mini-svg-data-uri": "^1.4.4",
    "node-fetch": "^2.6.7",
    "package-json": "^7.0.0",
    "prettier": "^2.6.2",
    "prompts": "^2.4.2",
    "react": "^18.1.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "tmp": "^0.2.1",
    "toposort": "^2.0.2",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "type-fest": "^2.13.0",
    "typescript": "^4.7.2",
    "verdaccio": "^5.10.2",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.9",
    "yargs": "^17.5.1",
    "zx": "^6.1.0"
  }
}
